{
  "name": "@gagandeep023/json-view",
  "version": "0.1.1",
  "description": "JSON formatting, diffing, and stringify parsing tools",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "json",
    "formatter",
    "diff",
    "stringify",
    "parser",
    "typescript"
  ],
  "author": "Gagandeep Bhatia <gagandeepbhatia2303@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gagandeep023/json-view.git"
  },
  "homepage": "https://github.com/Gagandeep023/json-view#readme",
  "bugs": {
    "url": "https://github.com/Gagandeep023/json-view/issues"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "eslint": "^9.39.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
